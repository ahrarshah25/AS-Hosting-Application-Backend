{
  "version": 2,
  "builds": [
    { "src": "api/config.js", "use": "@vercel/node" },
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "api/deploy.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/config", "dest": "/api/config.js" },
    { "src": "/api/upload", "dest": "/api/upload.js" },
    { "src": "/api/deploy", "dest": "/api/deploy.js" }
  ]
}
